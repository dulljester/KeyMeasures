<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="120dp"
    android:viewportHeight="24"
    android:viewportWidth="24"
    >
        <group
            android:name="clockGroup">
            <group
                android:name="groupMidnight"
                android:translateX="11"
                android:translateY="1"
                >
                <path
                    android:name="digit1Path"
                    android:strokeWidth="0.1"
                    android:strokeColor="@color/matteBlack"
                    android:pathData="@string/path_one"
                    />
                <group
                    android:name="group2"
                    android:translateX="0.5"
                    >
                    <path
                        android:name="digit2Path"
                        android:strokeWidth="0.1"
                        android:strokeColor="@color/matteBlack"
                        android:pathData="@string/path_two"
                        />
                </group>
            </group>

            <group
                android:name="groupThree"
                android:translateX="22"
                android:translateY="10"
                >
                <path
                    android:name="digit3Path"
                    android:strokeColor="@color/matteBlack"
                    android:strokeWidth="0.1"
                    android:pathData="@string/path_three"
                    />
            </group>

            <group
                android:name="groupSix"
                android:translateX="11"
                android:translateY="22"
                >
                <path
                    android:name="pathSix"
                    android:strokeWidth="0.1"
                    android:strokeColor="@color/matteBlack"
                    android:pathData="@string/path_six"
                    />
            </group>

            <group
                android:name="groupNine"
                android:translateX="1.5"
                android:translateY="10"
                >
                <path
                    android:name="digitNine"
                    android:strokeColor="@color/matteBlack"
                    android:strokeWidth="0.1"
                    android:pathData="@string/path_nine"
                    />
            </group>
        </group>
</vector>