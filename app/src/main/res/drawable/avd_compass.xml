<animated-vector
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/vd_compass"
    tools:targetApi="lollipop">
    <target android:name="test">
        <aapt:attr name="android:animation">
            <set android:ordering="sequentially">
                <objectAnimator
                    android:duration="140"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="0"
                    android:valueTo="45"
                    />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="45"
                    android:valueTo="-45"
                    />
                <objectAnimator
                    android:duration="250"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="-45"
                    android:valueTo="45"
                    />
                <objectAnimator
                    android:duration="230"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="45"
                    android:valueTo="-35"
                    />
                <objectAnimator
                    android:duration="230"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="-35"
                    android:valueTo="35"
                    />
                <objectAnimator
                    android:duration="200"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="35"
                    android:valueTo="-25"
                    />
                <objectAnimator
                    android:duration="200"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="-25"
                    android:valueTo="25"
                    />
                <objectAnimator
                    android:duration="200"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="25"
                    android:valueTo="-15"
                    />
                <objectAnimator
                    android:duration="200"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="-15"
                    android:valueTo="5"
                    />
                <objectAnimator
                    android:duration="200"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="5"
                    android:valueTo="-3"
                    />
                <objectAnimator
                    android:duration="200"
                    android:interpolator="@android:interpolator/fast_out_slow_in"
                    android:propertyName="rotation"
                    android:valueFrom="-3"
                    android:valueTo="0"
                    />
            </set>
        </aapt:attr>
    </target>
</animated-vector>
